<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_expire_meetings" model="ir.cron">
            <field name="name">Stajyer: Görüşme Süresi Kontrolü</field>
            <field name="model_id" ref="model_stajyer_meeting"/>
            <field name="state">code</field>
            <field name="code">model._check_meeting_status()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>

 
        </record>

        <record id="ir_cron_update_progress" model="ir.cron">
            <field name="name">Stajyer: İlerleme Güncelleme</field>
            <field name="model_id" ref="model_stajyer_takip"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_progress()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>


        </record>
    </data>
</odoo>